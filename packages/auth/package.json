{
  "name": "@app/auth",
  "version": "0.1.0",
  "description": "LTV moleculer auth service",
  "main": "index.js",
  "author": "Luc <luc@ltv.vn>",
  "license": "MIT",
  "scripts": {
    "serve": "export NODE_PATH=./ && node -r ts-node/register node_modules/.bin/moleculer-runner --hot --repl --envfile=.env.development services --mask **/*.service.ts --config moleculer.config.ts",
    "start:prod": "moleculer-runner --env --mask **/*.service.js --config moleculer.config.js",
    "test:unit": "jest --coverage",
    "build": "rimraf dist/* && webpack --node-args='--stack_size=6144 --max-old-space-size=6144' --progress --colors"
  },
  "resolutions": {
    "graphql": "14.6.0"
  },
  "dependencies": {
    "@slynova/flydrive": "^1.0.0-0",
    "@typegoose/typegoose": "^7.2.0",
    "aws-sdk": "^2.640.0",
    "bcrypt": "^3.0.6",
    "bcrypt-hashpass": "^0.1.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "dotenv": "^8.0.0",
    "email-templates": "^7.0.4",
    "exceljs": "^4.0.1",
    "file-type": "^14.6.2",
    "hashids": "^1.2.2",
    "helmet": "^3.21.2",
    "i18next": "^19.1.0",
    "i18next-node-fs-backend": "^2.1.3",
    "ioredis": "^4.14.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.6.0",
    "knex": "^0.20.13",
    "lodash.defaultsdeep": "^4.6.1",
    "lodash.flattendeep": "^4.4.0",
    "lodash.groupby": "^4.6.0",
    "lodash.isequal": "^4.5.0",
    "lodash.isfunction": "^3.0.9",
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^4.0.1",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "lodash.set": "^4.3.2",
    "lodash.unionby": "^4.8.0",
    "lodash.uniqby": "^4.7.0",
    "mkdirp": "^1.0.3",
    "moleculer": "^0.14.7",
    "moleculer-apollo-server": "^0.3.0",
    "moleculer-db": "^0.8.7",
    "moleculer-db-adapter-mongoose": "^0.8.4",
    "moleculer-decorators": "^1.2.0",
    "moleculer-web": "^0.9.1",
    "moment": "^2.24.0",
    "mongoose": "^5.9.18",
    "mongoose-hidden": "^1.9.0",
    "nats": "^1.4.0",
    "nodemailer": "^6.4.6",
    "nodemailer-html-to-text": "^3.1.0",
    "pug": "^2.0.4",
    "redlock": "^4.1.0",
    "semver": "^7.3.2",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@app/core": "*",
    "@app/types": "*",
    "@types/bcrypt": "^3.0.0",
    "@types/email-templates": "^6.0.3",
    "@types/faker": "^4.1.11",
    "@types/hashids": "^1.0.30",
    "@types/helmet": "^0.0.45",
    "@types/i18next-node-fs-backend": "^2.1.0",
    "@types/jest": "^25.1.4",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/lodash.defaultsdeep": "^4.6.6",
    "@types/lodash.flattendeep": "^4.4.6",
    "@types/lodash.groupby": "^4.6.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.isfunction": "^3.0.6",
    "@types/lodash.isobject": "^3.0.6",
    "@types/lodash.isstring": "^4.0.6",
    "@types/lodash.omit": "^4.5.6",
    "@types/lodash.pick": "^4.4.6",
    "@types/lodash.set": "^4.3.6",
    "@types/lodash.unionby": "^4.8.6",
    "@types/lodash.uniqby": "^4.7.6",
    "@types/mkdirp": "^1.0.0",
    "@types/mongodb": "^3.5.20",
    "@types/mongoose": "^5.7.22",
    "@types/node": "13.7.0",
    "@types/nodemailer": "^6.4.0",
    "copy-webpack-plugin": "^5.1.1",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "moleculer-repl": "^0.6.2",
    "nodemon": "^2.0.2",
    "ts-jest": "^25.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^utils/(.*)$": "<rootDir>/utils/$1",
      "^mixins/(.*)$": "<rootDir>/mixins/$1",
      "^services/(.*)$": "<rootDir>/services/$1",
      "^tests/(.*)$": "<rootDir>/tests/$1"
    },
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html",
      "lcov",
      "text"
    ],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "services/**/*.ts",
      "middlewares/**/*.ts",
      "mixins/**/*.ts",
      "utils/**/*.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "verbose": true,
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ]
  }
}
